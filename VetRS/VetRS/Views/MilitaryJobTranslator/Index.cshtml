@model IEnumerable<VetRS.Models.MilitaryJobTranslator>

@{
    ViewData["Title"] = "Military Job Translator";
}

<h1>Index</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MilitaryJobTitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CivilianJobTitle)
            </th>
            <th></th>
        </tr>
